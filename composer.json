{
    "name": "elegance/laravel-admin",
    "description": "laravel admin",
    "type": "library",
    "keywords": ["laravel", "admin", "table", "form", "tree"],
    "homepage": "https://github.com/explore-pu/laravel-admin",
    "license": "MIT",
    "authors": [
        {
            "name": "xiaozhi",
            "email": "xiaozhi2015@hotmail.com"
        }
    ],
    "require": {
        "symfony/dom-crawler": "^7.1"
    },
    "require-dev": {
        "laravel/laravel": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Elegance\\Admin\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elegance\\Admin\\AdminServiceProvider"
            ],
            "aliases": {
                "Admin": "Elegance\\Admin\\Facades\\Admin"
            }
        }
    }
}
