{
    "name": "elegant-utils/laravel-admin",
    "description": "laravel admin",
    "type": "library",
    "keywords": ["laravel", "admin", "table", "form", "tree"],
    "homepage": "https://github.com/explore-pu/laravel-admin",
    "license": "MIT",
    "authors": [
        {
            "name": "xiaozhi",
            "email": "xiaozhi2015@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "laravel/laravel": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Elegant\\Utils\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elegant\\Utils\\AdminServiceProvider"
            ],
            "aliases": {
                "Admin": "Elegant\\Utils\\Facades\\Admin"
            }
        }
    }
}
